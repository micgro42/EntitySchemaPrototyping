{
	"name": "WikibaseSchema",
	"version": "0.0.0",
	"author": [
		"The Wikidata team"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:WikibaseSchema",
	"descriptionmsg": "wikibaseschema-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "wikibase",
	"requires": {
		"MediaWiki": ">= 1.33.0"
	},
	"load_composer_autoloader": true,
	"AutoloadNamespaces": {
		"Wikibase\\Schema\\": "src/"
	},
	"ExtensionMessagesFiles": {
		"WikibaseSchemaAlias": "WikibaseSchema.i18n.alias.php"
	},
	"MessagesDirs": {
		"WikibaseSchema": [
			"i18n"
		]
	},
	"Hooks": {
		"SkinTemplateNavigation": "Wikibase\\Schema\\WikibaseSchemaHooks::onSkinTemplateNavigation",
		"LoadExtensionSchemaUpdates": "Wikibase\\Schema\\WikibaseSchemaHooks::onCreateDBSchema",
		"ExtensionTypes": "Wikibase\\Schema\\WikibaseSchemaHooks::onExtensionTypes",
		"BeforeDisplayNoArticleText": "\\Wikibase\\Schema\\WikibaseSchemaHooks::onBeforeDisplayNoArticleText",
		"PageHistoryLineEnding": "\\Wikibase\\Schema\\WikibaseSchemaHooks::onPageHistoryLineEnding"
	},
	"ContentHandlers": {
		"WikibaseSchema": "Wikibase\\Schema\\MediaWiki\\Content\\WikibaseSchemaContentHandler"
	},
	"namespaces": [
		{
			"id": 640,
			"constant": "NS_WBSCHEMA_JSON",
			"name": "Schema",
			"subpages": false,
			"content": true,
			"defaultcontentmodel": "WikibaseSchema"
		},
		{
			"id": 641,
			"constant": "NS_WBSCHEMA_JSON_TALK",
			"name": "Schema_talk",
			"subpages": true,
			"content": false,
			"defaultcontentmodel": "wikitext"
		}
	],
	"ResourceModules": {
		"ext.WikibaseSchema.view": {
			"styles": [ "viewSchema.less" ],
			"localBasePath": "src/MediaWiki/Resources",
			"remoteExtPath": "WikibaseSchema/src/MediaWiki/Resources"
		}
	},
	"SpecialPages": {
		"NewSchema": "Wikibase\\Schema\\MediaWiki\\Specials\\NewSchema"
	},
	"manifest_version": 2
}
