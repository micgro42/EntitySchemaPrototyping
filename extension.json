{
	"name": "WikibaseSchema",
	"version": "0.0.0",
	"author": [
		"The Wikidata team"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:WikibaseSchema",
	"descriptionmsg": "wikibaseschema-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "wikibase",
	"requires": {
		"MediaWiki": ">= 1.32.0"
	},
	"AutoloadNamespaces": {
		"Wikibase\\Schema\\": "src/"
	},
	"ExtensionMessagesFiles": {
		"WikibaseSchemaAlias": "WikibaseSchema.i18n.alias.php"
	},
	"MessagesDirs": {
		"WikibaseSchema": [
			"i18n"
		]
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": "Wikibase\\Schema\\WikibaseSchemaHooks::onCreateDBSchema",
		"ExtensionTypes": "Wikibase\\Schema\\WikibaseSchemaHooks::onExtensionTypes"
	},
	"ContentHandlers": {
		"WikibaseSchema": "Wikibase\\Schema\\MediaWiki\\Content\\WikibaseSchemaContentHandler"
	},
	"namespaces": [
		{
			"id": 12300,
			"constant": "NS_WBSCHEMA_JSON",
			"name": "Schema",
			"subpages": false,
			"content": true,
			"defaultcontentmodel": "WikibaseSchema"
		},
		{
			"id": 12301,
			"constant": "NS_WBSCHEMA_JSON_TALK",
			"name": "Schema_talk",
			"subpages": true,
			"content": false,
			"defaultcontentmodel": "wikitext"
		}
	],
	"SpecialPages": {
		"NewSchema": "Wikibase\\Schema\\MediaWiki\\Specials\\NewSchema"
	},
	"manifest_version": 2
}
