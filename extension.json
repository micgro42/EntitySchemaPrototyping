{
	"name": "WikibaseSchema",
	"author": [
		"The Wikidata team"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:WikibaseSchema",
	"descriptionmsg": "wikibaseschema-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "wikibase",
	"requires": {
		"MediaWiki": ">= 1.33.0"
	},
	"load_composer_autoloader": true,
	"AutoloadNamespaces": {
		"EntitySchema\\": "src/",
		"EntitySchema\\Tests\\": "tests/phpunit/"
	},
	"ExtensionMessagesFiles": {
		"WikibaseSchemaAlias": "WikibaseSchema.i18n.alias.php"
	},
	"MessagesDirs": {
		"WikibaseSchema": [
			"i18n"
		]
	},
	"Hooks": {
		"SkinTemplateNavigation": "EntitySchema\\MediaWiki\\WikibaseSchemaHooks::onSkinTemplateNavigation",
		"LoadExtensionSchemaUpdates": "EntitySchema\\MediaWiki\\WikibaseSchemaHooks::onCreateDBSchema",
		"ExtensionTypes": "EntitySchema\\MediaWiki\\WikibaseSchemaHooks::onExtensionTypes",
		"BeforeDisplayNoArticleText": "\\EntitySchema\\MediaWiki\\WikibaseSchemaHooks::onBeforeDisplayNoArticleText",
		"PageHistoryLineEnding": "\\EntitySchema\\MediaWiki\\WikibaseSchemaHooks::onPageHistoryLineEnding",
		"FormatAutocomments": "\\EntitySchema\\MediaWiki\\WikibaseSchemaHooks::onFormatAutocomments",
		"ContentModelCanBeUsedOn": "\\EntitySchema\\MediaWiki\\WikibaseSchemaHooks::onContentModelCanBeUsedOn",
		"ImportHandleRevisionXMLTag": "\\EntitySchema\\MediaWiki\\WikibaseSchemaHooks::onImportHandleRevisionXMLTag",
		"NamespaceIsMovable": "\\EntitySchema\\MediaWiki\\WikibaseSchemaHooks::onNamespaceIsMovable",
		"TitleGetRestrictionTypes": "\\EntitySchema\\MediaWiki\\WikibaseSchemaHooks::onTitleGetRestrictionTypes"
	},
	"ContentHandlers": {
		"WikibaseSchema": "EntitySchema\\MediaWiki\\Content\\WikibaseSchemaContentHandler"
	},
	"namespaces": [
		{
			"id": 640,
			"constant": "NS_ENTITYSCHEMA_JSON",
			"name": "EntitySchema",
			"subpages": false,
			"content": true,
			"defaultcontentmodel": "WikibaseSchema"
		},
		{
			"id": 641,
			"constant": "NS_ENTITYSCHEMA_JSON_TALK",
			"name": "EntitySchema_talk",
			"subpages": true,
			"content": false,
			"defaultcontentmodel": "wikitext"
		}
	],
	"config": {
		"WBSchemaSchemaTextMaxSizeBytes": {
			"value": 50000,
			"description": "Max length of the Schema text in bytes",
			"public": true
		},
		"WBSchemaNameBadgeMaxSizeChars": {
			"value": 250,
			"description": "Max length of the label description or combined aliases in characters",
			"public": true
		},
		"WBSchemaShExSimpleUrl": {
			"value": null,
			"description": "The URL for a shex-simple installation. Users are sent here to check entities against schema, with a schemaURL=â€¦ url query parameter appended to the URL specifying a URL for the schema text.",
			"public": true
		}
	},
	"ResourceModules": {
		"ext.WikibaseSchema.view": {
			"styles": [ "viewSchema.less" ],
			"localBasePath": "resources",
			"remoteExtPath": "WikibaseSchema/resources"
		},
		"ext.WikibaseSchema.special.setSchemaLabelDescriptionAliases.edit": {
			"scripts": [ "special.setSchemaLabelDescriptionAliases.edit.js" ],
			"dependencies": [
				"oojs-ui-widgets",
				"mediawiki.widgets.visibleLengthLimit"
			],
			"localBasePath": "resources",
			"remoteExtPath": "WikibaseSchema/resources"
		},
		"ext.WikibaseSchema.special.newSchema": {
			"scripts": [ "special.newSchema.js" ],
			"dependencies": [
				"oojs-ui-widgets",
				"mediawiki.widgets.visibleLengthLimit"
			],
			"localBasePath": "resources",
			"remoteExtPath": "WikibaseSchema/resources"
		},
		"ext.WikibaseSchema.action.edit": {
			"scripts": [ "action.edit.js" ],
			"dependencies": [
				"oojs-ui-widgets",
				"mediawiki.widgets.visibleLengthLimit"
			],
			"localBasePath": "resources",
			"remoteExtPath": "WikibaseSchema/resources"
		},
		"ext.WikibaseSchema.action.view.trackclicks": {
			"scripts": [ "action.view.trackclicks.js" ],
			"localBasePath": "resources",
			"remoteExtPath": "WikibaseSchema/resources"
		}
	},
	"SpecialPages": {
		"NewEntitySchema": "EntitySchema\\MediaWiki\\Specials\\NewEntitySchema",
		"EntitySchemaText": "EntitySchema\\MediaWiki\\Specials\\EntitySchemaText",
		"SetEntitySchemaLabelDescriptionAliases": "EntitySchema\\MediaWiki\\Specials\\SetEntitySchemaLabelDescriptionAliases"
	},
	"manifest_version": 2
}
